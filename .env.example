# Local desktop testing (HTTP)
VITE_SOCKET_URL=http://localhost:5000
VITE_PEER_HOST=localhost
VITE_PEER_PORT=5000
VITE_PEER_PATH=/peerjs/myapp
VITE_PEER_SECURE=false

